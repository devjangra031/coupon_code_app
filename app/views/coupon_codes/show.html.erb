<div class="page-header">
  <%= link_to coupon_codes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_coupon_code_path(@coupon_code), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show coupon_code</h1>
</div>

<dl class="dl-horizontal">
  <dt>Code:</dt>
  <dd><%= @coupon_code.code %></dd>

  <dt>Type:</dt>
  <dd><%= @coupon_code.type %></dd>

  <dt>Value:</dt>
  <dd><%= @coupon_code.value %></dd>

  <dt>Cashback value:</dt>
  <dd><%= @coupon_code.cashback_value %></dd>

  <dt>Start date:</dt>
  <dd><%= @coupon_code.start_date %></dd>

  <dt>End date:</dt>
  <dd><%= @coupon_code.end_date %></dd>

  <dt>Active:</dt>
  <dd><%= @coupon_code.active %></dd>

  <dt>Applicable outlet ids:</dt>
  <dd><%= @coupon_code.applicable_outlet_ids %></dd>

  <dt>Minimum delivery amount after discount:</dt>
  <dd><%= @coupon_code.minimum_delivery_amount_after_discount %></dd>

  <dt>Maximum discount:</dt>
  <dd><%= @coupon_code.maximum_discount %></dd>

</dl>
